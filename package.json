{
  "name": "rescue-dump-manager",
  "productName": "ETA rescue dump manager",
  "version": "2023.6.09",
  "description": "ETA rescue dump manager",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "reinstall": "yarn install",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Leo",
    "email": "eta.development.leo@outlook.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "rescue-dump-manager"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/main-rendered-window.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/external-bridge.ts"
                  }
                }
              ]
            },
            "port": 3003,
            "loggerPort": 9003
          }
    }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/plugin-webpack": "^6.1.1",
    "@types/adm-zip": "^0.5.0",
    "@types/eventsource": "^1.1.11",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "^6.8.1",
    "electron": "^25.1.0",
    "eslint": "^8.42.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.3",
    "svg-url-loader": "^8.0.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "adm-zip": "^0.5.10",
    "axios": "^1.4.0",
    "devextreme": "^23.1.3",
    "devextreme-react": "^23.1.3",
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "eventsource": "^2.0.2",
    "fetch-cookie": "^2.1.0",
    "formdata-node": "^4.3.3",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-router-dom": "^6.12.1",
    "tough-cookie": "^4.1.3",
    "ws": "^8.6.0"
  }
}
